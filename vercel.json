{
  "headers": [
    {
      "source": "/admin/fonts/pretendard/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/user/fonts/pretendard/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/admin/(.*)", "destination": "/apps/nowait-admin/$1" },
    { "source": "/user/(.*)", "destination": "/apps/nowait-user/$1" }
  ]
}
